{# src/btsappli/StagesBundle/Resources/views/Stages/rechercheEntreprise.html.twig #}

{% extends "::vueMere.html.twig" %}

{% block contenuCentral %}
<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading"><h4>Rechercher l'entreprise</h4></div>
        <div class="panel-body">
            <form method="post" {{ form_enctype(formulaireRecherche) }}>
                {{ form_widget(formulaireRecherche) }}
                <input type="submit" class="btn btn-primary" />
            </form>
        </div>
    </div>
</div>
{% endblock %}